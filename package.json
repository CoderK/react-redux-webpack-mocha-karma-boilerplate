{
  "name": "reactist-starter-kit",
  "version": "0.0.1",
  "description": "It's boilerplate repository for reactist, react cli.",
  "main": "index.js",
  "scripts": {
    "build": "webpack --progress",
    "build:watch": "node ./node_modules/webpack/bin/webpack.js --progress --watch",
    "build:production": "webpack -p --env production",
    "test": "node ./node_modules/karma/bin/karma start --headless",
    "test-debug": "node ./node_modules/karma/bin/karma start",
    "coverage": "node ./node_modules/karma/bin/karma start --coverage --headless",
    "deploy": "npm run coverage && npm run build:production",
    "start": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CoderK/reactist-starter-kit"
  },
  "keywords": [],
  "author": "jeokrang@gmail.com",
  "license": "MIT",
  "dependencies": {
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.7.4",
    "babel-preset-airbnb": "^1.1.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "chai": "^3.5.0",
    "enzyme": "^2.2.0",
    "istanbul-instrumenter-loader": "^0.2.0",
    "karma": "^0.13.22",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.2.3",
    "karma-cli": "^0.1.2",
    "karma-coverage": "^0.5.5",
    "karma-mocha": "^0.2.2",
    "karma-mocha-reporter": "^2.0.2",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.4.5",
    "phantomjs-prebuilt": "^2.1.7",
    "react-addons-test-utils": "^15.0.2",
    "redux-devtools": "^3.2.0",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  }
}
